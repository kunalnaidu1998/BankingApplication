<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SignUpController">
   <children>
      <HBox minHeight="-Infinity" minWidth="-Infinity">
         <children>
            <Label minHeight="-Infinity" minWidth="-Infinity" text="First Name:">
               <HBox.margin>
                  <Insets right="5.0" top="3.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="newFirstNameTextField">
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </TextField>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox minHeight="-Infinity" minWidth="-Infinity">
         <children>
            <Label minHeight="-Infinity" minWidth="-Infinity" text="Last Name:">
               <HBox.margin>
                  <Insets right="5.0" top="3.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="newLastNameTextField">
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </TextField>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox minHeight="-Infinity" minWidth="-Infinity">
         <children>
            <Label minHeight="-Infinity" minWidth="-Infinity" text="Username:">
               <HBox.margin>
                  <Insets right="5.0" top="3.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="newUsernameTextField">
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </TextField>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox minHeight="-Infinity" minWidth="-Infinity">
         <children>
            <Label minHeight="-Infinity" minWidth="-Infinity" text="Password:">
               <HBox.margin>
                  <Insets right="5.0" top="3.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="newPasswordTextField">
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </TextField>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <Label fx:id="signUpErrorLabel" minHeight="-Infinity" minWidth="-Infinity" textFill="RED">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </Label>
      <Button minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#SignUpButton" text="Sign Up">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
